{
	"apiVersion": 2,
	"name": "gutenberg-bento/fit-text",
	"title": "Fit Text",
	"category": "gutenberg-bento-blocks",
	"description": "Fit text to its container.",
	"keywords": ["text"],
	"textdomain": "default",
	"attributes": {
		"content": {
			"type": "string",
			"source": "html",
			"selector": "bento-fit-text",
			"default": "",
			"__unstablePreserveWhiteSpace": true,
			"__experimentalRole": "content"
		},
		"minFontSize": {
			"type": "string",
			"default": "10px"
		},
		"maxFontSize": {
			"type": "string",
			"default": "200px"
		},
		"height": {
			"type": "number",
			"default": 50
		},
		"textAlign": {
			"type": "string"
		}
	},
	"supports": {
		"align": ["full", "wide"]
	},
	"editorScript": "file:./fit-text.js",
	"editorStyle": "file:./view.css",
	"style": ["file:./view.css", "bento-fit-text"],
	"viewScript": "bento-fit-text"
}
