{
	"apiVersion": 2,
	"name": "gutenberg-bento/carousel",
	"title": "Carousel",
	"category": "gutenberg-bento-blocks",
	"description": "Display multiple images in a carousel.",
	"keywords": ["images", "photos"],
	"textdomain": "default",
	"attributes": {
	  "controls": {
		  "type": "string",
		  "default": "always"
	  },
	  "orientation": {
		  "type": "string",
		  "default": "horizontal"
	  },
	  "direction": {
		  "type": "string",
		  "default": "ltr"
	  },
	  "autoAdvance": {
		  "type": "boolean",
		  "default": false
	  },
	  "visibleCount": {
		  "type": "number",
		  "default": 1
	  },
	  "loop": {
		  "type": "boolean",
		  "default": false
	  },
	  "mixedLengths": {
		  "type": "boolean",
		  "default": true
	  },
	  "snap": {
		  "type": "boolean",
		  "default": true
	  }
	},
	"supports": {
		"align": true
	},
	"viewScript": "file:./carousel.view.js",
	"editorScript": "file:./carousel.js",
	"editorStyle": "file:./carousel.css",
	"style": ["file:./carousel.view.css", "bento-base-carousel"]
}
