{
	"apiVersion": 2,
	"name": "gutenberg-bento/socialshare",
	"title": "Social Share",
	"category": "gutenberg-bento-blocks",
	"description": "Displays a sharing button for social platforms or system share.",
	"keywords": [ "social", "share", "social networks" ],
	"textdomain": "default",
	"attributes": {
		"facebook": {
			"type": "boolean",
			"default": false
		},
		"twitter": {
			"type": "boolean",
			"default": true
		}
	},
	"viewScript": "bento-social-share",
	"editorScript": "file:./socialshare.js",
	"editorStyle": "file:./socialshare.css",
	"style": ["bento-social-share"]
}
